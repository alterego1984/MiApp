@page
@model MiApp.Presentacion.Pages.ListaPersonasModel
@{
}

<h1>Personas</h1>
    <table class="table">
        <tr>
            <th>
                Nombres
            </th>
            <th>
                Apellidos
            </th>
            <th>
                Edad
            </th>
            <th>
                Género
            </th>
            <th>
                Documento
            </th>
        </tr>
@foreach (var persona in Model.Personas)
{
    <tr>
        <td>@persona.Nombres</td>
        <td>@persona.Apellidos</td>
        <td>@persona.Edad</td>
        <td>@persona.Genero</td>
        <td>@persona.Documento</td>
    </tr>
}
    </table>
