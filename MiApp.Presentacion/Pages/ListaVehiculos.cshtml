@page
@model MiApp.Presentacion.Pages.ListaVehiculosModel
@{
}

<h1>Vehiculos</h1>
    <table class="table">
        <tr>
            <th>
                Placa
            </th>
            <th>
                Modelo
            </th>
            <th>
                Marca
            </th>
            <th>
                Accidente
            </th>
        </tr>
@foreach (var vehiculo in Model.Vehiculos)
{
    <tr>
        <td>@vehiculo.Placa</td>
        <td>@vehiculo.Modelo</td>
        <td>@vehiculo.Marca</td>
        <td>@vehiculo.AccidenteId</td>
    </tr>
}
    </table>
